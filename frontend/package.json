{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "^22.18.0"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "test:watch:coverage": "vitest --coverage",
    "check-format": "prettier --check \"src/**/*.{js,jsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.17",
    "@tanstack/react-table": "^8.21.3",
    "@testing-library/user-event": "^14.6.1",
    "@uiw/react-json-view": "^2.0.0-alpha.34",
    "axios": "^1.13.2",
    "bootstrap": "^5.3.8",
    "react": "^19.2.3",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.71.1",
    "react-router": "^7.12.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-alias": "^6.0.0",
    "@storybook/addon-links": "9.1.17",
    "@storybook/builder-vite": "9.1.17",
    "@storybook/react-vite": "9.1.17",
    "@storybook/react-webpack5": "9.1.17",
    "@stryker-mutator/vitest-runner": "^9.4.0",
    "@testing-library/react": "^16.3.1",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/eslint-plugin": "^1.6.6",
    "axios-mock-adapter": "^2.1.0",
    "chromatic": "^13.1.3",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-storybook": "9.1.17",
    "globals": "^17.0.0",
    "jsdom": "^27.4.0",
    "msw": "^2.12.7",
    "msw-storybook-addon": "^2.0.6",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "storybook": "9.1.17",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_|",
          "varsIgnorePattern": "(^_)|(^React$)"
        }
      ],
      "react/react-in-jsx-scope": "off",
      "react/jsx-uses-react": "off",
      "react/prop-types": "off",
      "react/no-unescaped-entities": "off",
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "(^_)|(^React$)"
        }
      ]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
}
